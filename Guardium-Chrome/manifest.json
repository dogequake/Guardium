{
  "manifest_version": 3,
  "name": "GuardiumProxy",
  "version": "1.0.0.0",
  "description": "Подключение к прокси-серверу",
  "permissions": [
    "proxy",
    "storage",
    "notifications",
    "sidePanel"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "default_title": "Guardium"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "side_panel": {
    "default_path": "panel/panel.html"
  }
}
